WRITE A PROGRAM TO PRINT THE SQUARE OF ANY NUMBER ........

Using systerm;
Using namespace;
class program
{
void print()
{
Console.WriteLine("CODE OF THE SQUARE OF ANY NUMBER");

int num,Square;
Console.WriteLine("Enter the any number");
num=Convert.ToInt32(Console.ReadLine());
Square = num*num;
Console.WriteLine("square of number is -: "+Square);
}
Public static void Main(string[] args)
{
Program p=new Program();
p.print();
Console.ReadLine();

}

}
